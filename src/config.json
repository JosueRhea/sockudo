{
  "debug": true,
  "port": 6001,
  "host": "0.0.0.0",
  "app_manager.driver": "memory",
  "app_manager.array.apps": [
    {
      "id": "app1",
      "key": "key1",
      "secret": "secret1",
      "enable_client_messages": true,
      "enabled": true,
      "max_connections": "100",
      "max_client_events_per_second": "10",
      "webhooks" : [
        {
          "url": "http://localhost:3000/webhook",
          "event_types": ["member_added", "member_removed"]
        }
      ]
    },
    {
      "id": "app2",
      "key": "key2",
      "secret": "secret2",
      "enable_client_messages": false,
      "enabled": true,
      "max_connections": "50",
      "max_client_events_per_second": "5"
    }
  ],
  "adapter.driver": "redis",
  "adapter.redis.prefix": "sockudo",
  "adapter.redis.requests_timeout": "5000",
  "cache.driver": "redis",
  "cache.redis.url": "redis://localhost:6379",

  "webhooks.enabled": true,
  "webhooks.queue.driver": "redis",
  "webhooks.queue.redis.url": "redis://localhost:6379",
  "webhooks.queue.redis.prefix": "sockudo",
  "webhooks.queue.redis.concurrency": "5",
  "webhooks.batching.enabled": false,
  "webhooks.batching.duration": "50"
}